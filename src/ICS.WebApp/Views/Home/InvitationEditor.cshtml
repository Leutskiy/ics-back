<div id="editor" class="section panel panel-primary">
    <div class="panel-heading">
        Создание приглашения
    </div>
    <div class="panel-body">
        <div class="form-group">
            <label>Наименование</label>
            <input class="form-control" data-bind="value: model.editor.name" />
        </div>
        <div class="form-group">
            <label>Цель поездки</label>
            <input class="form-control" data-bind="value: model.editor.visitGoal" />
        </div>
        <div class="form-group">
            <label>Имя иностранца</label>
            <input class="form-control" data-bind="value: model.editor.alienName" />
        </div>
        <div class="form-group">
            <label>Страна поезки</label>
            <input class="form-control" data-bind="value: model.editor.visitCountry" />
        </div>
        <button class="btn btn-primary" data-bind="click: handleSaveClick">
            Сохранить
        </button>
    </div>
</div>